								<VBox>
									<Label text="{i18n>BulkMaterial}" id="ID_BOM_SCHGT_TXT" tooltip="{i18n>BulkMaterial}" class="cl_inputLabel"></Label>
									<CheckBox id="ID_BOM_SCHGT" selected="{JM_BOMTable>Schgt}" select="fnCheckBoxLiveChange" class="cl_opr_checkbx"
										editable="{= ${JM_ContextModel>/Ind} !== 'T' }"></CheckBox>
									<layoutData>
										<grid:GridData span="L1 M6 S6"/>
									</layoutData>
								</VBox>
								<VBox>
									<Label text="{i18n>BlkMatIndMatMst}" id="ID_BOM_SCHKZ_TXT" tooltip="{i18n>BlkMatIndMatMstDes}" class="cl_inputLabel"></Label>
									<CheckBox id="ID_BOM_SCHKZ" selected="{JM_BOMTable>Schkz}" select="fnCheckBoxLiveChange" class="cl_opr_checkbx" editable="false"
										enabled="false"></CheckBox>
									<layoutData>
										<grid:GridData span="L2 M6 S6"/>
									</layoutData>
								</VBox>
							</grid:Grid>
						</VBox>
					</IconTabFilter>
					<IconTabFilter text="{i18n>Administration}">
						<grid:Grid class="cl_item_gridRemovePadding">
							<HBox height="1.5rem" class="sapUiTinyMarginBottom sapUiTinyMarginTop cl_subheading">
								<Label text="{i18n>AdminstrativeData}" class="sapUiTinyMarginBegin cl_subheadingFont"></Label>
								<layoutData>
									<grid:GridData span="L12 M12 S12"/>
								</layoutData>
							</HBox>
							<VBox>
								<Label text="{i18n>ChangeNumber}" id="ID_BOM_AENNR_TXT" tooltip="{i18n>ChangeNumber}" class="cl_inputLabel"></Label>
								<VBox width="100%">
									<Input id="ID_BOM_AENNR" value="{JM_BOMTable>Aennr}" class="cl_inputField" liveChange="fnLiveChange"
										editable="{= ${JM_ContextModel>/Ind} !== 'T' }"></Input>
								</VBox>
								<layoutData>
									<grid:GridData span="L3 M12 S12"/>
								</layoutData>
							</VBox>
							<VBox>
								<Label text="{i18n>ToChangeNumber}" id="ID_BOM_AENRA_TXT" tooltip="{i18n>ToChangeNumber}" class="cl_inputLabel"></Label>
								<VBox width="100%">
									<Input id="ID_BOM_AENRA" value="{JM_BOMTable>Aenra}" class="cl_inputField" liveChange="fnLiveChange"
										editable="{= ${JM_ContextModel>/Ind} !== 'T' }"></Input>
								</VBox>
								<layoutData>
									<grid:GridData span="L3 M12 S12"/>
								</layoutData>
							</VBox>
							<VBox>
								<Label text="{i18n>ValidFrom}" tooltip="{i18n>ValidFrom}" class="cl_inputLabel"></Label>
								<VBox width="100%">
									<DatePicker value="{ path: 'JM_BOMTable>Datuv',formatter: '.formatter.fnDateFormater' }" displayFormat="dd.MM.yyyy"
										valueFormat="yyyy-MM-ddTHH:mm:ss" placeholder=" " change="fnDateRangevalidation" class=" cl_inputField cl_customDatePicker"
										tooltip="{ path: 'JM_BOMTable>Datuv',formatter: '.formatter.fnDateFormater' }" editable="false"/>
								</VBox>
								<layoutData>
									<grid:GridData span="L3 M12 S12"/>
								</layoutData>
							</VBox>
							<VBox>
								<Label text="{i18n>ValidTo}" tooltip="{i18n>ValidTo}" class="cl_inputLabel"></Label>
								<VBox width="100%">
									<DatePicker value="{ path: 'JM_BOMTable>Datub',formatter: '.formatter.fnDateFormater' }" displayFormat="dd.MM.yyyy"
										valueFormat="yyyy-MM-ddTHH:mm:ss" placeholder=" " change="fnDateRangevalidation" class="cl_inputField cl_customDatePicker"
										tooltip="{ path: 'JM_BOMTable>Datub',formatter: '.formatter.fnDateFormater' }" editable="false"/>
								</VBox>
								<layoutData>
									<grid:GridData span="L3 M12 S12"/>
								</layoutData>
							</VBox>
							<VBox>
								<Label text="{i18n>CreatedOn}" tooltip="{i18n>CreatedOn}" class="cl_inputLabel"></Label>
								<VBox width="100%">
									<DatePicker value="{ path: 'JM_BOMTable>Andat',formatter: '.formatter.fnDateFormater' }" displayFormat="dd.MM.yyyy"
										valueFormat="yyyy-MM-ddTHH:mm:ss" placeholder=" " change="fnDateRangevalidation" class="cl_inputField cl_customDatePicker"
										tooltip="{ path: 'JM_BOMTable>Andat',formatter: '.formatter.fnDateFormater' }" editable="false"/>
								</VBox>
								<layoutData>
									<grid:GridData span="L3 M12 S12"/>
								</layoutData>
							</VBox>
							<VBox>
								<Label text="{i18n>CreatedBy}" tooltip="{i18n>CreatedBy}" class="cl_inputLabel"></Label>
								<VBox width="100%">
									<Input value="{JM_BOMTable>Annam}" class="cl_inputField" liveChange="fnLiveChange" editable="false"></Input>
								</VBox>
								<layoutData>
									<grid:GridData span="L3 M12 S12"/>
								</layoutData>
							</VBox>
							<VBox>
								<Label text="{i18n>ChangedOn}" tooltip="{i18n>ChangedOn}" class="cl_inputLabel"></Label>
								<VBox width="100%">
									<DatePicker value="{ path: 'JM_BOMTable>Aedat',formatter: '.formatter.fnDateFormater' }" displayFormat="dd.MM.yyyy"
										valueFormat="yyyy-MM-ddTHH:mm:ss" change="fnDateRangevalidation" class="cl_inputField cl_customDatePicker"
										tooltip="{ path: 'JM_BOMTable>Aedat',formatter: '.formatter.fnDateFormater' }" editable="false"/>
								</VBox>
								<layoutData>
									<grid:GridData span="L3 M12 S12"/>
								</layoutData>
							</VBox>
							<VBox>
								<Label text="{i18n>ChangedBy}" tooltip="{i18n>ChangedBy}" class="cl_inputLabel"></Label>
								<VBox width="100%">
									<Input value="{JM_BOMTable>Aenam}" class="cl_inputField" liveChange="fnLiveChange" editable="false"></Input>
								</VBox>
								<layoutData>
									<grid:GridData span="L3 M12 S12"/>
								</layoutData>
							</VBox>
						</grid:Grid>
					</IconTabFilter>
					<IconTabFilter text="{i18n>DocumentAssignment}">
						<RadioButtonGroup select="fnDocumentRadiolvl" id="id_radioButton" columns="2"
							class="cl_radiobutton sapUiTinyMarginTop sapUiSmallMarginBegin">
							<RadioButton id="id_currVersion" text="Current Version" class="cl_radiobutton"/>
							<RadioButton id="id_allVersion" text="All Version" class="cl_radiobutton" selected="true"/>
						</RadioButtonGroup>
						<VBox id="id_documentTable">
							<HBox height="1.5rem" class="sapUiSmallMarginBeginEnd sapUiTinyMarginBottom " justifyContent="SpaceBetween">
								<Label text="{i18n>LinkedDocuments}" class="sapUiTinyMarginBegin cl_tableHeaderTxt"></Label>
								<HBox>
									<Image class="cl_pv_removeImgFocus cl_imagePos sapUiSmallMarginEnd" width="1.6rem" press="fnAddRow" tooltip="{i18n>Add}"
										src="{= ${JM_ImageModel>/path} + 'PlusBtn.svg'}"/>
									<Image class="cl_pv_removeImgFocus cl_imagePos sapUiSmallMarginEnd" width="1.6rem" press="fnDeleteRow" tooltip="{i18n>Delete}"
										src="{= ${JM_ImageModel>/path} + 'DeleteBtn.svg'}"/>
								</HBox>
								<layoutData>
									<grid:GridData span="L12 M12 S12"/>
								</layoutData>
							</HBox>
							<VBox class="sapUiSmallMarginBeginEnd sapUiSmallMarginBottom sapUiTinyMarginTop">
								<uitable:Table id="idDynamicUITable" rows="{JM_DocumentModel>/DocumentList}" visibleRowCount="4" selectionMode="Single" class="cl_opr_Tbl">
									<uitable:columns>
										<uitable:Column>
											<Label text="{i18n>DocumentType}"/>
											<uitable:template>
												<VBox>
													<Input value="{JM_DocumentModel>DocType}" showValueHelp="true" class="cl_bom_tableInput"></Input>
												</VBox>
											</uitable:template>
										</uitable:Column>
										<uitable:Column>
											<Label text="{i18n>Document}"/>
											<uitable:template>
												<VBox>
													<Input value="{JM_DocumentModel>Document}" showValueHelp="true" class="cl_bom_tableInput"></Input>
												</VBox>
											</uitable:template>
										</uitable:Column>
										<uitable:Column>
											<Label text="{i18n>DocumentVersion}"/>
											<uitable:template>
												<VBox class="cl_item_tablenoneditBg">
													<Input value="{JM_DocumentModel>Document}" editable="false" class="cl_bom_tableInput"></Input>
												</VBox>
											</uitable:template>
										</uitable:Column>
										<uitable:Column>
											<Label text="{i18n>DocumentPart}"/>
											<uitable:template>
												<VBox class="cl_item_tablenoneditBg">
													<Input value="{JM_DocumentModel>Document}" editable="false" class="cl_bom_tableInput"></Input>
												</VBox>
											</uitable:template>
										</uitable:Column>
									</uitable:columns>
								</uitable:Table>
							</VBox>
						</VBox>
					</IconTabFilter>
				</items>
			</IconTabBar>
		</VBox>
		<buttons>
			<Button text="{i18n>Continue}" tooltip="{i18n>Continue}" class="sapUiSizeCompact cl_primaryButton sapUiSmallMarginEnd"
				icon="{= ${JM_ImageModel>/path} + 'Apply.svg'}" press="fnItemSubmit"/>
		</buttons>
	</Dialog>
