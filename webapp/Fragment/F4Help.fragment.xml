<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog title="{i18n>F4Dialog}" class="cl_f4Help sapUiSizeCompact" verticalScrolling="false" afterClose="fnAfterCloseFragment">
		<VBox class="sapUiSmallMargin cl_f4searchbox" width="80%" height="3rem">
			<SearchField liveChange="fnValueSearch" width="100%" placeholder="{i18n>SearchHere}" search="fnValueSearch" class="cl_f4searchfield"/>
		</VBox>
		<VBox class="cl_f4tableScroll">
			<Table id="idMaterialTable" items="{JM_F4Model>/rows}" mode="None" inset="false" fixedLayout="false" width="100%"
				class="sapUiResponsiveMargin cl_f4table">
				<headerToolbar >
					<Toolbar visible="false">
						<Title text="{i18n>F4Dialog}" level="H2"/>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column visible="{= !!${JM_F4Model>/labels/col1} }">
						<Text text="{JM_F4Model>/labels/col1}"/>
					</Column>
					<Column visible="{= !!${JM_F4Model>/labels/col2} }">
						<Text text="{JM_F4Model>/labels/col2}"/>
					</Column>
					<Column visible="{= !!${JM_F4Model>/labels/col3} }">
						<Text text="{JM_F4Model>/labels/col3}"/>
					</Column>
					<Column visible="{= !!${JM_F4Model>/labels/col4} }">
						<Text text="{JM_F4Model>/labels/col4}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem type="Active" press="fnF4Itempress">
						<cells>
							<Text text="{JM_F4Model>col1}" visible="{= !!${JM_F4Model>col1} }"/>
							<Text text="{JM_F4Model>col2}" visible="{= !!${JM_F4Model>col2} }"/>
							<Text text="{JM_F4Model>col3}" visible="{= !!${JM_F4Model>col3} }"/>
							<Text text="{JM_F4Model>col4}" visible="{= !!${JM_F4Model>col4} }"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</VBox>
		<endButton>
			<Button text="{i18n>Cancel}" class="sapUiSizeCompact cl_f4table_change sapUiSmallMarginEnd"  icon="{= ${JM_ImageModel>/path} + 'Cancel.svg'}" press="fnf4HelpCancel"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>