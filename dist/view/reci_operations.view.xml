										<VBox width="35%" class="cl_inputFieldGap">
											<Input editable="false" value="{JM_KeydataModel>/Plnal}" class="cl_inputField" maxLength="4"></Input>
										</VBox>
										<layoutData>
											<grid:GridData span="L3 M12 S12"/>
										</layoutData>
									</HBox>
									<layoutData>
										<grid:GridData span="L3 M12 S12"/>
									</layoutData>
								</VBox>
							</grid:Grid>
							<VBox class="cl_reci_dupDialog_tableBox sapUiSmallMarginTop">
