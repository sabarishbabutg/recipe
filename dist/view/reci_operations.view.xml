															value="{ parts: [ { path: 'JM_ReciOperations>Vornr' }, { path: 'JM_EditableModel>' }, { value: 'Phflg' } ], formatter: '.formatter.fnGetSateForOperaTbl' }"
															writeToDom="true"/>
													</customData>
												</VBox>
											</uitable:template>
										</uitable:Column>
										<uitable:Column filterProperty="Pvznr" sortProperty="Pvznr" tooltip="{i18n>SupOperation}" width="7rem">
