											<Label text="{i18n>Operation}" tooltip="{i18n>Operations}"/>
											<uitable:template>
												<VBox>
													<Input value="{JM_ReciOperations>Vornr}" class="cl_opr_tableInput" maxLength="4"
														editable="{ parts : [{path : 'JM_ReciOperations>Vornr'}, { path: 'JM_EditableModel>' }, { value: 'Vornr' }], formatter: '.formatter.fnEditableState' }"/>
													<customData>
														<core:CustomData key="flag"
															value="{ parts: [ { path: 'JM_ReciOperations>Vornr' }, { path: 'JM_EditableModel>' }, { value: 'Vornr' } ], formatter: '.formatter.fnGetSateForOperaTbl' }"
															writeToDom="true"/>
													</customData>
												</VBox>
											</uitable:template>
										</uitable:Column>
										<uitable:Column filterProperty="Phflg" sortProperty="Phflg" tooltip="{i18n>Phase}" width="3.4rem">
