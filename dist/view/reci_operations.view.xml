									<Label text="{i18n>ResultList}" class="sapUiTinyMarginBegin cl_search_tableHeaderTxt"></Label>
									<Label text="{}" class="sapUiTinyMarginBegin cl_search_tableCntTxt"></Label>
									<ToolbarSpacer/>
									<Button id="id_DeselectAll" text="{i18n>DeselectAll}" class="cl_opr_DeselectBtn sapUiSizeCompact" press="fnDeselectAll"/>
