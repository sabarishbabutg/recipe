												<VBox>
													<CheckBox selected="{JM_ReciOperations>Phflg}" class="cl_opr_checkbx" select="fnPhaseSelect"
														editable="{ parts : [{path : 'JM_ReciOperations>Vornr'}, { path: 'JM_EditableModel>' }, { value: 'Phflg' }], formatter: '.formatter.fnEditableState' }"/>
													<customData>
