											<Input value="{JM_KeydataModel>/Werks}" class="cl_inputField" editable="false" maxLength="4"></Input>
										</VBox>
										<VBox width="70%">
											<Input width="100%" value="{JM_KeydataModel>/WerksDes}" class="cl_inputDescField" editable="false"></Input>
										</VBox>
										<layoutData>
											<grid:GridData span="L3 M12 S12"/>
										</layoutData>
									</HBox>
									<layoutData>
										<grid:GridData span="L3 M12 S12"/>
									</layoutData>
								</VBox>
								<VBox>
									<Label text="{i18n>RecipeGroup}" class="cl_inputLabel"/>
									<HBox>
										<VBox width="23%" class="cl_inputFieldGap">
											<Input showValueHelp="true" value="{JM_KeydataModel>/Plnnr}" editable="false" class="cl_inputField" maxLength="4"></Input>
										</VBox>
										<VBox width="77%">
											<Input width="100%" value="{JM_KeydataModel>/ReciDes}" class="cl_inputDescField" editable="false"></Input>
										</VBox>
										<layoutData>
											<grid:GridData span="L3 M12 S12"/>
