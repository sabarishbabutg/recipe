<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="MANAGED_RECIPE.controller.reci_operations"
	xmlns:html="http://www.w3.org/1999/xhtml" xmlns:grid="sap.ui.layout" xmlns:uitable="sap.ui.table">
	<App>
		<pages>
			<Page showHeader="true" title="Application" class="cl_pageBg sapUiSizeCompact" enableScrolling="false">
				<customHeader>
					<OverflowToolbar class="cl_header_toolbar">
						<Image src="{= ${JM_ImageModel>/path} + 'NavBack.svg'}" tooltip="{i18n>navBack}" width="1.5rem" press="fnBackConfirmation"
							class="cl_pv_removeImgFocus sapUiSmallMarginBegin"/>
						<Title id="id_title" text="{}" class="cl_header_title"/>
						<ToolbarSpacer/>
						<!--added by srikanth-->
						<Label text="{JM_UserModel>/0/Agent} - {JM_UserModel>/0/Uname}" class="cl_userid_font sapUiSmallMarginEnd"/>
					</OverflowToolbar>
				</customHeader>
				<!--<core:Fragment fragmentName="MANAGED_RECIPE.Fragment.NavigationBar" type="XML"/>-->
				<content>
					<VBox>
						<VBox class="sapUiSmallMarginBeginEnd sapUiTinyMarginTop">
							<grid:Grid class="cl_search_grid">
